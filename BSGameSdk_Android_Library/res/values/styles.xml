        <item name="android:ems">8</item>
        <item name="android:textColor">@color/bsgamesdk_text_info_color</item>
        <item name="android:gravity">left|center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:singleLine">true</item>
    </style>
    
    <style name="bsgamesdk_checkbox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:button">@drawable/bsgamesdk_check_box</item>
        <item name="android:paddingLeft">4dp</item>
        <item name="android:textColor">@color/bsgamesdk_text_agree_color</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="Bili.animStyle" parent="@android:style/Widget.ProgressBar.Large">
