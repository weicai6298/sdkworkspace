    <style name="bsgamesdk_editText_basic" parent="@android:style/Widget.EditText">
        <item name="android:textSize">@dimen/bsgamesdk_input_text_size</item>
        <item name="android:background">@drawable/bsgamesdk_bg_input</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:ellipsize">end</item>
        <item name="android:paddingBottom">8dp</item>
