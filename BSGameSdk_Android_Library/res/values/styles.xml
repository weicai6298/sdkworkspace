        <item name="android:windowExitAnimation">@anim/bsgamesdk_to_top</item>  
    </style>  
    
    
    <style name="bsgemsdk_activity_welcome_anim" parent="@android:style/Animation.Activity">
        <item name="android:activityOpenEnterAnimation">@anim/bsgamesdk_from_top</item>
    	<item name="android:activityOpenExitAnimation">@anim/bsgamesdk_to_top</item>
    	<item name="android:activityCloseEnterAnimation">@anim/bsgamesdk_to_top</item>
   		<item name="android:activityCloseExitAnimation">@anim/bsgamesdk_to_top</item> 
    </style>

    <style name="bsgamesdk_textView_arraw">
