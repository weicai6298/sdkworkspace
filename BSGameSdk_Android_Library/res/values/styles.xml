        <item name="android:paddingLeft">8dp</item>
        <item name="android:ems">8</item>
        <item name="android:textColor">@color/bsgamesdk_text_color</item>
        <item name="android:gravity">left|center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:singleLine">true</item>
    </style>

    
        <style name="bsgamesdk_item_user_textView" parent="@android:style/Widget.TextView">
        <item name="android:textSize">@dimen/bsgamesdk_input_text_size</item>
