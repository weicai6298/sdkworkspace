<?xml version="1.0" encoding="utf-8"?>
<!-- 只读属性的输入编辑框  修改器专用-->
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
	<item android:state_enabled="false" > <!-- 实际上正在输入 -->
       	<shape> 
       	    <solid   android:color="#ffc6e6c4"/>
            <corners android:radius="3dp" />
            <stroke  android:width="2dp" android:color="#ff41ad3c" />
        </shape>
    </item> 
    
    <item android:state_enabled="true" >
       	<shape>
            <solid android:color="#f0f0f0f0" />
            <corners android:radius="3dp" />
            <stroke  android:width="1dp" android:color="#ff41ad3c" />
        </shape>
    </item>


</selector>