<resources>

    <!-- Base application theme. -->
    <style name="sy233AppTheme" parent="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen">

    </style>

    <style name="sy233transTheme" parent="sy233AppTheme">
        <item name="android:windowBackground">@android:color/transparent</item>
        　　
        　　
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="Mdialog" parent="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen">
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowEnterAnimation">@anim/sy233pop_in_anim</item>
        <item name="android:windowExitAnimation">@anim/sy233pop_out_anim</item>
    </style>

    <style name="sy233register_edit">
        <item name="android:layout_marginLeft">8dp</item>
        <item name="android:layout_marginRight">8dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:textColorHint">@color/sy233hintextcolor</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/sy233blacktextcolor</item>
        <item name="android:background">@null</item>
        <item name="android:textCursorDrawable">@null</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="sy233loading_dialog" parent="android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@drawable/sy233loading_bg</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="sy233popwin_anim_style">
        <item name="android:windowEnterAnimation">@anim/sy233pop_in_anim</item>
        <item name="android:windowExitAnimation">@anim/sy233pop_out_anim</item>
    </style>

    <style name="sy233popwin_trans_anim_style">
        <item name="android:windowEnterAnimation">@anim/sy233pop_trans_in_anim</item>
        <item name="android:windowExitAnimation">@anim/sy233pop_trans_out_anim</item>
    </style>

    <style name="sy233line1px">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:background">#e4e4e4</item>
    </style>

</resources>
